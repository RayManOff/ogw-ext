{
  "name": "Ogw-Shortener",
  "version": "1.0",
  "description": "Link Shortening",
  "manifest_version": 2,
  "permissions": [
    "http://ogw.ru/*",
    "tabs"
  ],
  "background" : {
    "scripts" : [
      "src/background/message.js",
      "src/background/background.js"
    ],
    "persistent" : false
  },
  "browser_action": {
    "default_title": "Ogw Shortener",
    "default_popup": "src/popup/html/popup.html"
  }
}